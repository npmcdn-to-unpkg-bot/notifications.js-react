{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "rules": {
    "semi": ["error", "always"],
    "space-before-function-paren": 0,
    "comma-dangle": ["error", "always-multiline"]
  },
  "plugins": [
    "react"
  ]
}
